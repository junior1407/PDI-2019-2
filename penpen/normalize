image = cv2.imread("lenacolor512.tiff", cv2.IMREAD_COLOR)  # uint8 image
norm_image = cv2.normalize(image, None, alpha=0, beta=1, norm_type=cv2.NORM_MINMAX, dtype=cv2.CV_32F)
#define 	CV_8U   0
 
#define 	CV_8S   1
 
#define 	CV_16U   2
 
#define 	CV_16S   3
 
#define 	CV_32S   4
 
#define 	CV_32F   5
 
#define 	CV_64F   6
